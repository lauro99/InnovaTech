{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Link",
          "value": "<https://www.innovatechmex.com>; rel=canonical"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://www.innovatechmex.com/$1",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "innova-tech-beige.vercel.app"
        }
      ]
    }
  ]
}
